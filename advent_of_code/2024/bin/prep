#!/usr/bin/env bash

set -euo pipefail
DIR="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )/.." &> /dev/null && pwd)"
cd "$DIR"

DAY=$(printf "%02d" $1)

cat <<EOF > src/t/day$DAY.clj
(ns t.day$DAY
  (:require [t.lib :as lib :refer [->long]]))

(defn parse
  [lines]
  lines)

(defn solve
  [lines]
  0)

(defn part1
  [input]
  (solve input))

(defn part2
  [input]
  (solve input))

(lib/check
  [part1 sample] 1
  [part1 puzzle] 1
  [part2 sample] 1
  [part2 puzzle] 1)
EOF
